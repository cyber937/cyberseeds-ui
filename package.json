{
  "name": "cyberseeds-ui",
  "version": "1.1.1",
  "license": "MIT",
  "keywords": [
    "react-components"
  ],
  "homepage": "https://github.com/cyber937/cyberseeds-ui",
  "type": "module",
  "files": [
    "dist",
    "dist/style.css"
  ],
  "main": "dist/cyberseeds-ui.js",
  "module": "dist/cyberseeds-ui.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/cyberseeds-ui.js",
      "default": "./dist/cyberseeds-ui.js"
    },
    "./style.css": {
      "default": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build:css:preview": "tailwindcss -i ./src/index.css -o ./src/style.css --minify",
    "build:css": "tailwindcss -i ./src/index.css -o ./dist/style.css --minify",
    "build": "npm run build:css:preview && tsc -b && vite build && npm run build:css",
    "lint": "eslint .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:storybook": "vitest --config vitest.config.ts",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:deploy": "gh-pages -d storybook-static"
  },
  "peerDependencies": {
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "clsx": "^2.1.1",
    "@eslint/js": "^9.25.0",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/addon-vitest": "^10.2.8",
    "@storybook/react": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@tailwindcss/cli": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.2.2",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/ui": "^3.2.2",
    "axe-core": "^4.10.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^10.2.8",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "jest-axe": "^9.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.52.0",
    "storybook": "^10.2.8",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.2"
  },
  "overrides": {
    "storybook": "$storybook"
  }
}
